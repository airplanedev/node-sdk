{
  "name": "airplane",
  "version": "0.2.0-15",
  "description": "Node.js SDK for writing Airplane.dev tasks",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "airplanedev/node-sdk",
  "author": "hello@airplane.dev",
  "license": "MIT",
  "scripts": {
    "test": "jest --testTimeout 500",
    "test:watch": "jest --watch --testTimeout 500",
    "build": "rm -rf dist && mkdir dist && cp -t dist LICENSE README.md package.json && tsc -p tsconfig.build.json",
    "lint": "eslint --ext js,ts,yml,yaml,json --max-warnings=0 .",
    "lint:fix": "eslint --ext js,ts,yml,yaml,json --max-warnings=0 --fix ."
  },
  "engines": {
    "node": "^14 || ^16 || ^18"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/plugin-transform-modules-commonjs": "7.18.6",
    "@types/jest": "28.1.4",
    "@types/node": "16.11.43",
    "@types/node-fetch": "2.6.2",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "babel-jest": "28.1.2",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-yaml": "0.5.0",
    "husky": "8.0.1",
    "jest": "28.1.2",
    "lint-staged": "13.0.3",
    "nock": "13.2.8",
    "prettier": "2.7.1",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.(ts|js|json|yaml|yml)": "eslint --fix"
  },
  "prettier": {
    "printWidth": 100
  },
  "dependencies": {
    "cross-fetch": "3.1.5",
    "fetch-retry": "5.0.3",
    "query-string": "7.1.1",
    "temporalio": "0.23.2",
    "uuid": "8.3.2"
  }
}
