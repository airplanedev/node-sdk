{
  "name": "airplane",
  "version": "0.2.0-4",
  "description": "Node.js SDK for writing Airplane.dev tasks",
  "main": "./dist/index.js",
  "repository": "airplanedev/node-sdk",
  "author": "hello@airplane.dev",
  "license": "MIT",
  "scripts": {
    "test": "jest --testTimeout 500",
    "test:watch": "jest --watch --testTimeout 500",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "lint": "eslint --ext js,ts,yml,yaml,json --max-warnings=0 .",
    "lint:fix": "eslint --ext js,ts,yml,yaml,json --max-warnings=0 --fix .",
    "prepare": "husky install",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^14 || ^16 || ^18"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/plugin-transform-modules-commonjs": "7.17.9",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/node-fetch": "2.6.1",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "babel-jest": "27.5.1",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-yaml": "0.5.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.8",
    "nock": "13.2.4",
    "np": "7.6.1",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "*.(ts|js|json|yaml|yml)": "eslint --fix"
  },
  "prettier": {
    "printWidth": 100
  },
  "dependencies": {
    "cross-fetch": "3.1.5",
    "fetch-retry": "5.0.2",
    "query-string": "7.1.1",
    "uuid": "8.3.2"
  },
  "np": {
    "message": "Release v%s",
    "preview": true
  }
}
